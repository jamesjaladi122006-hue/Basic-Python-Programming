#positive or negative
a = int(input("Enter the value of a"))
if(a>0):
    print("a is positive")
elif(a<0):
    print("a is negative")
else:
    print("zero")


#even or odd
a = int(input("Enter the value of a"))
if(a%2==0):
    print("a is an even number")
else:
    print("a is an odd number")

