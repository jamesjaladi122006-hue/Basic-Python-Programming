#passed or not
a = int(input("Enter the value of a"))
b = int(input("Enter the value of b"))
total=a+b
if(a>=35 and b>=15 and total>=50):
    print("passed")
else:
    print("not pased")

#age program
age=int(input("Enter the age"))
if(age<18):
    print("person is child")
elif(age>=18 and age<=50):
    print("person is Adult")
elif(age>50):
    print("perosn is old")
else:
    print("not a human")


#grade
name=input("enter the name")
marks=int(input("enter the marks"))
if(marks>=90):
    print("A")
    print("EXCELLENT")
elif(marks>=80 and marks<=89):
    print("B")
    print("GOOD")
elif(marks>=70 and marks<=79):
    print("C")
    print("NOT BAD")
elif(marks>=60 and marks<=69):
    print("D")
    print(" BAD")
else:
    print("FAILED")


#days in a week
day=int(input("enter the number"))
if(day==1):
        print("MONDAY")
if(day==2):
        print("TUESDAY")
if(day==3):
        print("WEDNESDAY")
if(day==4):
        print("THURSDAY")
if(day==5):
        print("FRIDAY")
if(day==6):
        print("SATURDAY")
if(day==7):
        print("SUNDAY")


