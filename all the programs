# swapinfg of two numbers without 3rd variable
a=int(input("enter first number"))
b=int(input("enter second number"))
a=a+b
b=a-b
a=a-b
print("after swaping:",a)
print("after swaping:",b)

# swapinfg of two numbers with 3rd variable
a=int(input("enter"))
b=int(input("enter"))
c=a
a=b
b=c
print(a)
print(b)

