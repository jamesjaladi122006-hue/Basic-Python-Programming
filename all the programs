#temperature
temp=float(input("enter the value of temperature"))
if(temp<37):
    print("BODY IS COLD")
elif(temp==37):
    print("BODY IS NORMAL")
else:
    print("BODY IS FEVER")

#lunch program
def sum_of_cubes(m,n):
    total=0
    st = min(m,n)
    en = max(m,n)
    for i in range(st,en+1):
        total=total+i**3
     return total
print(sum_of_cubes(9,2))

#logical operators
a = 10
b = 5


print("AND example:")
print(a > 5 and b < 10) 
print(a > 15 and b < 10) 


print("\nOR example:")
print(a > 5 or b > 10)  
print(a < 5 or b > 10)   

#non primitive
fruits = ["apple", "banana", "cherry"]
print("List example:", fruits)


colors = ("red", "green", "blue")
print("Tuple example:", colors)

numbers = {1, 2, 3, 2}
print("Set example (no duplicates):", numbers)


person = {"name": "Alice", "age": 25}
print("Dictionary example:", person)

